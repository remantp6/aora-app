{
  "expo": {
    "name": "aora-app",
    "slug": "aora-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "aora-app-scheme",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161621"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.newlookorg.aora-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#161621"
      },
      "package": "com.newlookorg.aora-app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "package": "com.newlookorg.aora-app"
    },
    "plugins": ["expo-router",
    [
      "expo-local-authentication",
      {
        "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID to authenticate user."
      }
    ],
    "expo-video"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
